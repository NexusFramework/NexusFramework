<!DOCTYPE html>
<html><head><title>{{title}}</title>
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <meta charset="utf-8" /><meta name="robots" content="{{meta.robots || 'index; follow'}}" />
    <meta name="viewport" content="{{meta.viewport || 'width=device-width, initial-scale=1'}}" />

    <?add level "sys"?>
    <?add level "module"?>
    <?add level "plugin"?>
    <?add level "theme"?>
    <?add level ""?>

    <?add gfonts "Montserrat"?>
    <?add gfonts "Open Sans"?>

    <?map sysstyles //maxcdn.bootstrapcdn.com/font-awesome/4.2.0/css/font-awesome.min.css?>
    <?map sysstyles //maxcdn.bootstrapcdn.com/bootstrap/3.3.1/css/bootstrap.min.css?>

    <?map sysscripts //ajax.googleapis.com/ajax/libs/jquery/2.1.1/jquery.min.js?>
    <?map sysscripts //maxcdn.bootstrapcdn.com/bootstrap/3.3.1/js/bootstrap.min.js?>
    <?map sysscripts //cdn.jsdelivr.net/velocity/1.1.0/velocity.min.js?>
    <!--[if lt IE 9]>
        <script src="//cdnjs.cloudflare.com/ajax/libs/html5shiv/3.7.2/html5shiv.min.js"></script>
    <![endif]-->
    <?each env.gfonts?>
        <link rel="stylesheet" href="//fonts.googleapis.com/css?family={{{entry}}}" />
    <?done?>
    <?each env.level?>
        <?set key entry+"styles"?>
        <?each env[env.key]?>
            <link rel="{{entry.value.rel || 'stylesheet'}}" media="{{entry.value.media || 'all'}}" href="{{entry.key}}" />
        <?done?>
    <?done?>
</head><body class="{{env.bodyclass || ''}}">
    <?include env.body || "body.nhp"?>
    
    <?each env.level?>
        <?set key entry+"scripts"?>
        <?each env[env.key]?>
            <script src="{{entry.key}}" type="{{entry.type || 'text/javascript'}}"></script>
        <?done?>
    <?done?>
</body></html>